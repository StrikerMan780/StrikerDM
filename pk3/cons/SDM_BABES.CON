state babe_check_all_rescued
	ifvarvare BABES_RESCUED BABES_TOTAL
	ifvare BABES_ALLSAVED 0
	{
		redefinequote 500 ^08All babes saved!
		userquote 500
		globalsound ALL_BABES_SAVED
		setvar BABES_ALLSAVED 1
		
		headspritestat CURSPRITE STAT_PLAYER
		whilevarn CURSPRITE -1
		{
			getactor[CURSPRITE].extra TEMP
			ifvarg TEMP 0
			ifvarl TEMP MAXPLAYERHEALTH
			{
				setactor[CURSPRITE].extra MAXPLAYERHEALTH
			}
				
			nextspritestat CURSPRITE CURSPRITE
		}
	}
ends

state babe_add_rescued
	ifmultiplayer
	ifvarand gametype_flags 1 { } // GAMETYPE_COOP
	else
	{
		break
	}
	
	addvar BABES_RESCUED 1
	
	redefinequote 500 ^12Babe saved!^08 %d ^12/^08 %d
	qsprintf 500 500 BABES_RESCUED BABES_TOTAL
	userquote 500
ends

state babe_rescue_genericsound
	randvar RANDOM_SOUND 1
	switch RANDOM_SOUND
		case 0: sound BABE_SAVED01 break
		case 1: sound BABE_SAVED02 break
	endswitch
ends

state babe_rescue_sounds
	getactor[THISACTOR].picnum TEMP
	switch TEMP
		case FEM10:
			sound FEM10_SAVED
			break
		case NAKED1:
		case PODFEM1:
		case FEM5:
		case FEM6:
		case TOUGHGAL:
			// Nothing
			break
		default:
			state babe_rescue_genericsound
			break
	endswitch
ends

defstate duke_dead_babe_response
	ifp pfacing
	ifpdistl 1280
	ifhitspace
	{
		getplayer[THISACTOR].i TEMP
		randvar RANDOM_SOUND 2
		switch RANDOM_SOUND
			case 0: actorsound TEMP DUKE_BABEDEAD01 break
			case 1: actorsound TEMP DUKE_BABEDEAD02 break
			case 2: actorsound TEMP DUKE_HIT_STRIPPER2 break
		endswitch
	}
ends

defstate duke_rescued_babe_response
	ifvare TARGET -1
		break
	
	ifactor FEM10 break
	ifactor NAKED1 break
	ifactor PODFEM1 break
	ifactor FEM5 break
	ifactor FEM6 break
	ifactor TOUGHGAL break
	
	dist TEMP TARGET THISACTOR
	ifvarl TEMP 1280
	{
		randvar RANDOM_SOUND 8
		switch RANDOM_SOUND
			case 0: actorsound TARGET DUKE_BABEREPLY01 break
			case 1: actorsound TARGET DUKE_BABEREPLY02 break
			case 2: actorsound TARGET DUKE_BABEREPLY03 break
			case 3: actorsound TARGET DUKE_BABEREPLY04 break
			case 4: actorsound TARGET DUKE_BABEREPLY05 break
			case 5: actorsound TARGET DUKE_BABEREPLY06 break
			case 6: actorsound TARGET DUKE_BABEREPLY07 break
			case 7: actorsound TARGET DUKE_BABEREPLY08 break
			case 8: actorsound TARGET DUKE_BABEREPLY09 break
		endswitch
	}
ends

state rescueme
	ifactor NAKED1
	{
		ifinwater
		{
			state duke_dead_babe_response
			break // Drowned, can't be saved.
		}
	}
	else ifactor FEM8
	{
		state duke_dead_babe_response
		break // Impaled, can't be saved.
	}
	
	ifaction FEMSHRUNK break
	ifaction FEMGROW break
	ifaction FEMFROZEN1 break
	ifaction FEMFROZEN2 break
	
	ifmove FEMWAITRESPAWN
		break
	
	ifmove FEMRESCUED
	{
		ifcount 31 { }
		else ifcount 30
		{
			state babe_rescue_sounds
		}
		
		ifcount 69 { }
		else ifcount 68
		{
			ifrnd 32
				state duke_rescued_babe_response
		}
		
		ifcount 75
		{
			// Telepor Sparkles
			espawn BABETELEPORT
			gettiledata[PICNUM_MEMORY].xsize TEMP2
			mulvarvar TEMP2 XREPEAT_MEMORY
			shiftvarr TEMP2 6
			
			gettiledata[PICNUM_MEMORY].ysize TEMP3
			mulvarvar TEMP3 YREPEAT_MEMORY
			shiftvarr TEMP3 6
			
			ifvarvarg TEMP2 TEMP3
			{
				setactor[RETURN].xrepeat TEMP2
				setactor[RETURN].yrepeat TEMP2
			}
			else
			{
				setactor[RETURN].xrepeat TEMP3
				setactor[RETURN].yrepeat TEMP3
			}
			
			setactor[RETURN].blend 255
			setactor[RETURN].shade -127
			setactor[RETURN].cstat 2
			setvar RETURN 0
			
			ifactor PODFEM1 { }
			else
			{
				// Beam Out Effect
				espawn BABE_BEAM
				setactor[RETURN].shade -127
				setactor[RETURN].mdflags 16
				setactor[RETURN].pal 33 // White
				setactor[RETURN].xrepeat XREPEAT_MEMORY
				setactor[RETURN].yrepeat YREPEAT_MEMORY
				setactorvar[RETURN].PICNUM_MEMORY PICNUM_MEMORY
				
				ifactor FEM2
					setactorvar[RETURN].PICNUM_MEMORY FEM2NOPOLE
				else ifactor FEM5
					setactorvar[RETURN].PICNUM_MEMORY FEM5NOPOLE
				else ifactor FEM6
					setactorvar[RETURN].PICNUM_MEMORY FEM6NOPAD
				else ifactor NAKED1
					setactorvar[RETURN].PICNUM_MEMORY NAKED1NOVINE
				
				getactor[THISACTOR].cstat TEMP
				ifvarand TEMP 1
					xorvar TEMP 1
				ifvarand TEMP 256
					xorvar TEMP 256
				setactor[RETURN].cstat TEMP
				setvar RETURN 0
			}
			
			state babe_check_all_rescued
			
			respawnhitag
			resetcount
			move FEMWAITRESPAWN
			
			state babe_give_items
			
			ifactor FEM5
			{
				espawn CLEANPOLE
				setactor[RETURN].xrepeat XREPEAT_MEMORY
				setactor[RETURN].yrepeat YREPEAT_MEMORY
				setactor[RETURN].cstat CSTAT_MEMORY
				setactorvar[RETURN].DESPAWN_TYPE 2
				setvar RETURN 0
				state babe_hide_or_destroy
			}
			else ifactor FEM6
			{
				espawn EMPTYPAD
				setactor[RETURN].xrepeat XREPEAT_MEMORY
				setactor[RETURN].yrepeat YREPEAT_MEMORY
				setactor[RETURN].cstat CSTAT_MEMORY
				setactorvar[RETURN].DESPAWN_TYPE 2
				setvar RETURN 0
				state babe_hide_or_destroy
			}
			else ifactor PODFEM1
			{
				espawn EMPTYPOD
				setactor[RETURN].xrepeat XREPEAT_MEMORY
				setactor[RETURN].yrepeat YREPEAT_MEMORY
				setactor[RETURN].cstat CSTAT_MEMORY
				setactorvar[RETURN].DESPAWN_TYPE 2
				setvar RETURN 0
				state babe_hide_or_destroy
			}
			else ifactor NAKED1
			{
				espawn VINES
				setactor[RETURN].xrepeat XREPEAT_MEMORY
				setactor[RETURN].yrepeat YREPEAT_MEMORY
				
				getactor[THISACTOR].cstat TEMP
				ifvarand TEMP 1 { subvar TEMP 1 }
				ifvarand TEMP 256 { subvar TEMP 256 }
				setactor[RETURN].cstat TEMP
				
				setactorvar[RETURN].DESPAWN_TYPE 2
				setvar RETURN 0
				state babe_hide_or_destroy
			}
			else ifactor FEM2
			{
				espawn FEM2POLE
				setactor[RETURN].xrepeat XREPEAT_MEMORY
				setactor[RETURN].yrepeat YREPEAT_MEMORY
				setactor[RETURN].cstat CSTAT_MEMORY
				setactorvar[RETURN].DESPAWN_TYPE 2
				setvar RETURN 0
				state babe_hide_or_destroy
			}
			else
			{
				state babe_hide_or_destroy
			}
		}
		
		break // Rescued, so don't run the below test again
	}
	
	ifp pfacing
	ifhitspace
	{
		setvar TEMP 0
		ifactor NAKED1 // Extend the use range for the hanging babe sprites
		{
			getplayer[THISACTOR].i TEMP2
			ldist TEMP3 THISACTOR TEMP2
			ifvarl TEMP3 1280
			{
				ifpdistl 2048
					setvar TEMP 1
			}
		}
		else
		{
			ifpdistl 1280
				setvar TEMP 1
		}
		
		ifvarn TEMP 1
			break
			
		ifvare TARGET -1
			getplayer[THISACTOR].i TARGET
			
		resetcount
		state babe_add_rescued
		move FEMRESCUED
	}
ends


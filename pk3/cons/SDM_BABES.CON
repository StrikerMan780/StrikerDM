state femcode
	state babe_wait_respawn
	
	ifmove FEMRESCUED
		break
	
	ifactor NAKED1 { }
	else ifactor FEM6 { }
	else
	{
		fall
	}

	ifaction FEMSHRUNK
	{
		ifcount SHRUNKDONECOUNT
		{
			action FEMANIMATE
			cstat 257
		}
		else ifcount SHRUNKCOUNT
		{
			sizeto 40 40
		}
		else
		{
			state genericshrunkcode
		}
	}
	else ifaction FEMGROW
	{
		ifcount 32
		{
			respawnhitag
			guts JIBS4 20
			guts JIBS6 20
			spritepal 6
			
			ifactor NAKED1
			{
				debris SCRAP3 4

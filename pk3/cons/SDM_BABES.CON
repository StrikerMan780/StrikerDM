action FEMANIMATE

move FEMRESCUED
move FEMWAITRESPAWN

state babe_wait_respawn
	ifmultiplayer
	{
		ifvarand gametype_flags 1 { } // GAMETYPE_COOP
		else ifmove FEMWAITRESPAWN
		{
			ifcount BABERESPAWNTIME
			{
				setvar TARGET -1
				
				setactor[THISACTOR].picnum PICNUM_MEMORY
				setactor[THISACTOR].cstat CSTAT_MEMORY
				setactor[THISACTOR].extra TOUGH
				setactor[THISACTOR].xrepeat XREPEAT_MEMORY
				setactor[THISACTOR].yrepeat YREPEAT_MEMORY
				setactor[THISACTOR].pal PAL_MEMORY
				
				espawn TRANSPORTERSTAR
				gettiledata[PICNUM_MEMORY].ysize TEMP
				getactor[THISACTOR].z TEMP2
				mulvarvar TEMP YREPEAT_MEMORY
				shiftvarl TEMP 1
				subvarvar TEMP2 TEMP
				setactor[RETURN].z TEMP2
				setactor[RETURN].pal 1
				setactor[RETURN].shade -127
				setactor[RETURN].blend 255
				setvar RETURN 0
				
				resetcount
				move 0
				
				switch PICNUM_MEMORY
					case TOUGHGAL: action TOUGHGALANIM break
					case FEM10: action FEMANIMATESLOW break
					default: action FEMANIMATE break
				endswitch
			}
			
			return // Stop execution until we respawn
		}
	}
ends

state babe_hide_or_destroy
	move FEMWAITRESPAWN
	ifmultiplayer
	{
		ifvarand gametype_flags 1 // GAMETYPE_COOP
		{
			killit
		}
		else
		{
			cstat 32768
		}
	}
	else
	{
		killit
	}
ends

